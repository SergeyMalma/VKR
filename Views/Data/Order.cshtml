@using MyWebApp.Models

@{
    ViewBag.Title = "ASP.NET Identity";
}

<div class="panel panel-primary">
    <div class="panel-heading">
        Заказы
    </div>
    <table class="table table-striped">
        <tr>
            <th>Id</th>
            <th>Имя</th>
            <th>Номер заказа</th>
            <th></th>
        </tr>
        @foreach (Order order in Model)
        {
            <tr>
                <td>@order.Id</td>
                <td>@order.Name</td>
                <td>@order.OrderID</td>
            </tr>
        }
    </table>
</div>